<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
  <title>BeeHub Documentation</title>
  <parser>
    <target>doc-parser</target>
    <default-package-name>BeeHub</default-package-name>
  </parser>
  <transformer>
    <target>public/system/phpdoc</target>
  </transformer>
  <logging>
    <level>quiet</level>
  </logging>
  <files>
    <directory>src</directory>
    <directory>views</directory>
  </files>
</phpdoc>
